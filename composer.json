{
    "name": "karelbartunek/clockify",
    "type": "project",
    "autoload": {
        "psr-4": {
            "KarelBartunek\\Clockify\\": "src/"
        },
        "exclude-from-classmap": [
            "JDecool\\Clockify\\Api\\User\\User",
            "JDecool\\Clockify\\Model\\TimeEntryDtoImpl"
        ],
        "files": [
            ".hotfix/JDecool/Clockify/Api/User/User.php",
            ".hotfix/JDecool/Clockify/Model/TimeEntryDtoImpl.php"
        ]
    },
    "authors": [
        {
            "name": "Karel Bartunek"
        }
    ],
    "require": {
        "symfony/console": "^6.1",
        "symfony/yaml": "^6.1",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "jdecool/clockify-api": "dev-master",
        "symfony/debug": "^4.4",
        "symfony/orm-pack": "^2.2",
        "symfony/runtime": "^6.1",
        "symfony/dotenv": "^6.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "nette/neon": "^3.3"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.47",
        "edgedesign/phpqa": "^1.26",
        "phpstan/phpstan-symfony": "^1.2",
        "enlightn/security-checker": "^1.10",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-doctrine": "^1.3",
        "slevomat/coding-standard": "^8.6"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
